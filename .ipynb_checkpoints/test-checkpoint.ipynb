    "r = 2\n",
    "N = 1000000\n",
    "count_inside_circle = 0\n",
    "\n",
    "for i in range(N):\n",
    "    x_rand = random.uniform(0,r)\n",
    "    y_rand = random.uniform(0,r)\n",
    "    count_inside_circle += ((x_rand - 1) ** 2 + (y_rand - 1) ** 2 <= (r / 2) ** 2)\n",
    "\n",
    "(count_inside_circle/N)*4"
   ]
